train_net: "models/houzzdata/VGG16/faster_rcnn_end2end_houzzdata1/train.prototxt"
#base_lr: 0.001
base_lr: 0.0001
lr_policy: "step"
gamma: 0.1
# stepsize: 68000
#stepsize: 98000
stepsize: 45100
display: 20
average_loss: 100
# iter_size: 1
momentum: 0.9
weight_decay: 0.0005
# We disable standard caffe solver snapshotting and implement our own snapshot
# function
snapshot: 0
# We still use the snapshot prefix, though
snapshot_prefix: "vgg16_faster_rcnn"
iter_size: 2
